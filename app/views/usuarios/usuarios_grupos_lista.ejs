<%
if (typeof data != 'undefined' && data.length > 0) {
  for (var i = data.length - 1; i >= 0; i--) {
    %>
    <tr>
      <td class="center-align">
        <a href="/sistema/usuarios/grupos/ver/<%= data[i].id %>" class="ajax-load">
          <%= data[i].nome %>
        </a>
      </td>
    </tr>
    <%
  }
} else {
  %>
  <tr>
    <td class="center-align">Nenhuma resultado foi encontrado.</td>
  </tr>
  <%
}
%>